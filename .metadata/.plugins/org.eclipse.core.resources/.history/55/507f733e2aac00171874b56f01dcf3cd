package variableapp;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
			
		//Variable 
		int x = 10 ; 
		int y = 5 ; 
		int z ;
		z=x+y ;
	
		System.out.println("The result of the ( collection ) process = "+z);
		
		z=x-y ;
		
		System.out.println("The result of the ( subtraction ) process = "+z);
		
		z=x*y ;
		
		System.out.println("The result of the ( multiplication ) process = "+z);
		
		z=x/y ;
		
		System.out.println("The result of the ( Divide ) process = "+z);
		
		
	}

}
